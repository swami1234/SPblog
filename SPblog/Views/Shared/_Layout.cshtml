
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="MyWork Resume">
    <meta name="keywords" content="resume bootstrap template, resume / cv html template, html5 / css3 resume" />
    <meta name="author" content="KingStudio.ro">
    <!-- favicon -->
    <link rel="icon" href="images/images/favicon.png">
    <!-- page title -->
    <title>MyWork - Single Post</title>
    <!-- bootstrap css -->

    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <!-- css -->

    <link href="~/Content/style.css" rel="stylesheet" />

    <link href="~/Content/animate.css" rel="stylesheet" />
    <link href="~/Content/owl.carousel.css" rel="stylesheet" />
    <link href="~/Content/owl.theme.css" rel="stylesheet" />
    <link href="~/Content/PagedList.css" rel="stylesheet" />


    <!-- fonts -->
    <link href="https://fonts.googleapis.com/css?family=Merriweather" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:400,700" rel="stylesheet">
    <link href="~/Content/font-awesome.min.css" rel="stylesheet" />

    <link href="~/fonts/fonts/FontAwesome.otf" />

    <link href="~/Content/linear-icons.css" rel="stylesheet" />
    <link href="~/Content/Custom.css" rel="stylesheet" />

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
        <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
        <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->

</head>

<body class="main-demo single single-post">
    <!-- wrapper -->
    <div id="wrapper">
        <!-- side navigation -->

        <div id="sidebar-wrapper" style="background-image:url(/images/images/plain.jpg)">
            <ul class="sidebar-nav">
                <li class="sidebar-brand">
                    <a href="#"><img src="~/images/images/ccc.jpg" alt=""></a>
                </li>

                <li class="active">
                    <a class="menu-item page-scroll " href="https://localhost:44365/Home/Index"><h3 style="color:dodgerblue;">Home</h3></a>
                </li>
                <li>
                    <a class="menu-item page-scroll" href="@Url.Action("Contact","Home","Contact")"><h5>Contact-Me</h5></a>

                </li>
                <li>
                    <a class="menu-item page-scroll" href="@Url.Action("About","Home","About")"><h5>About-Me</h5></a>
                </li>
                @*@if ( !User.Identity.IsAuthenticated)
        {
             <li>
            <a class="menu-item page-scroll" href="https://localhost:44365/Account/Login"><h5>Login</h5></a>
        </li>
        <li>
            <a class="menu-item page-scroll" href="https://localhost:44365/Account/Register"><h5>New User? Please Register </h5></a>
        </li>
        }*@

                <li>
                    <a class="menu-item page-scroll" href="@Url.Action("Login","Account","Login")"><h5>Login</h5></a>
                </li>
                <li>
                    <a class="menu-item page-scroll" href="@Url.Action("Register","Account","Register")"><h5>New User? Please Register </h5></a>
                </li>


            </ul>
        </div>
        <!-- / side navigation -->
     
        <!-- content -->
        <div id="page-content-wrapper" class="header">
            <div class="container-fluid">
                @RenderBody()

                <footer>
                    <div class="container">
                        <p class="footer-info">
                            © 2016 - <strong>MyWork</strong> - Bootstrap Resume Template.
                            <span class="social pull-right">
                                <a href="#"><i class="fa fa-facebook"></i></a>
                                <a href="#"><i class="fa fa-twitter"></i></a>
                                <a href="#"><i class="fa fa-google-plus"></i></a>
                                <a href="#"><i class="fa fa-linkedin"></i></a>
                                <a href="#"><i class="fa fa-dribbble"></i></a>
                            </span>
                        </p>
                    </div><!-- / container -->
                </footer><!-- / light-footer -->
                <!-- scroll to top -->
                <a href="#top" class="scroll-to-top page-scroll is-hidden" data-nav-status="toggle"><i class="fa fa-angle-up"></i></a>
                <!-- / scroll to top -->
            </div><!-- / container-fluid -->
        </div><!-- / page-content-wrapper -->
    </div><!-- / wrapper -->
    <!-- javascript -->

    <script src="~/js/js/jquery.min.js"></script>
    <script src="~/js/js/bootstrap.min.js"></script>
    <script src="~/js/js/jquery.easing.min.js"></script>
    <!-- menu toggle script -->
    <script>
        $("#menu-toggle").click(function (e) {
            e.preventDefault();
            $("#wrapper").toggleClass("toggled");
        });</script>
    <!-- / menu toggle script -->
    <!-- nav scroll -->
    <script src="~/js/js/scrolling-nav.js"></script>
    <script>
        $(".sidebar-nav a").on("click", function () {
            $(".sidebar-nav").find(".active").removeClass("active");
            $(this).parent().addClass("active");
        });</script>
    <!-- / nav scroll -->
    <!-- contact form -->
    <script src="~/js/js/validator.min.js" type="text/javascript"></script>
    <script src="~/js/js/form-scripts.js" type="text/javascript"></script>
    <!-- / contact form -->
    <!-- facts counter -->
    <script src="~/js/js/jquery.countTo.js"></script>
    <script type="text/javascript">
        $('.timer').countTo({
            refreshInterval: 60,
            formatter: function (value, options) {
                return value.toFixed(options.decimals);
            },
        });</script>
    <!-- / facts counter -->
    <!-- portfolio script -->
    <script src="~/js/js/jquery.shuffle.min.js"></script>
    <script src="~/js/js/custom.js"></script>
    <!-- / portfolio script -->
    <!-- posts carousel -->
    <script src="~/js/js/owl.carousel.min.js"></script>

    <script>
        $(document).ready(function () {
            $("#posts-carousel").owlCarousel({
                items: 3,
                itemsDesktop: [1199, 3],
                itemsDesktopSmall: [979, 3]
            });

        });</script>
    <!-- / posts carousel -->
    <!-- hide nav -->
    <script src="~/js/js/hide-nav.js"></script>
    @RenderSection("scripts", required: false)
    <!-- / hide nav -->
    <!-- / javascript -->
</body>
</html>